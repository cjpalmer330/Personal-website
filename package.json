{
  "name": "codecademy-app",
  "homepage": "https://chris-palmer.me",
  "version": "0.1.0",
  "private": true,
  "resolutions": {
    "babel-eslint": "^10.1.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "postcss-cli": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.4",
    "react-scripts": "^5.0.1",
    "typescript": "^4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preinstall": "npx npm-force-resolutions"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.1",
    "tailwindcss": "^3.4.15"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "tailwindcss": {},
      "autoprefixer": {},
      "postcss-preset-env": {
        "stage": 1,
        "features": {
          "nesting-rules": true
        }
      },
      "cssnano": {
        "preset": [
          "default",
          {
            "calc": false,
            "transformProperties": false
          }
        ]
      }
    }
  }
}
