{
  "name": "vite-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "build:css": "tailwindcss -i ./src/input.css -o ./src/output.css",
    "build:min": "vue-tsc -b && vite build --mode production",
    "build:post": "tailwindcss -i ./src/input.css -o ./src/output.css --minify"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.5",
    "@tailwindcss/vite": "^4.1.5",
    "tailwindcss": "^4.1.5",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "wrangler": "^4.14.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/runtime-core": "^3.5.13",
    "@vue/runtime-dom": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vue-tsc": "^2.2.10"
  }
}
